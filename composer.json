{
    "name": "visual1au/statamic-phone-fieldtype",
    "description": "A Statamic fieldtype for phone number validation and E.164 formatting using Laravel-Phone with Australia-first detection",
    "type": "statamic-addon",
    "keywords": [
        "statamic",
        "fieldtype",
        "phone",
        "validation",
        "e164",
        "laravel-phone"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Visual1",
            "email": "support@visual1.com.au",
            "homepage": "https://visual1.com.au"
        }
    ],
    "require": {
        "php": "^8.1",
        "statamic/cms": "^4.0|^5.0",
        "propaganistas/laravel-phone": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Visual1\\StatamicPhoneFieldType\\": "src/"
        }
    },
    "extra": {
        "statamic": {
            "name": "Phone Number Fieldtype",
            "description": "A phone number fieldtype with validation and E.164 formatting using Australia-first detection"
        },
        "laravel": {
            "providers": [
                "Visual1\\StatamicPhoneFieldType\\ServiceProvider"
            ]
        }
    },
    "homepage": "https://github.com/visual1au/statamic-phone-fieldtype",
    "support": {
        "issues": "https://github.com/visual1au/statamic-phone-fieldtype/issues",
        "source": "https://github.com/visual1au/statamic-phone-fieldtype"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
